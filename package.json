{
  "name": "@philips-software/qubekit",
  "version": "1.0.0",
  "packageManager": "yarn@4.5.0",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "main": "dist/index.js",
    "types": "dist/index.d.ts"
  },
  "scripts": {
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "lint": "yarn eslint src",
    "build": "tsc",
    "format": "prettier --write \"**/*.ts\"",
    "format-check": "prettier --check \"**/*.ts\"",
    "merge-schemas": "openapi-merge-cli --config src/schema/merge.json",
    "openapi-ts": "yarn merge-schemas && openapi-ts"
  },
  "dependencies": {
    "@hey-api/client-fetch": "^0.10.2"
  },
  "devDependencies": {
    "@hey-api/openapi-ts": "^0.67.6",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.17",
    "eslint": "^9.27.0",
    "jest": "^29.7.0",
    "msw": "^2.8.4",
    "openapi-merge-cli": "^1.3.2",
    "prettier": "^3.5.3",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/philips-software/qubekit.git"
  }
}
